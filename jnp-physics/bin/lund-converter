echo "*    Running JNP Utils                  *"
echo "*    Version : 1a  Release : 2017       *" 
echo "*****************************************"
echo " "
echo " "
java -Xms1024m -cp "$LIBRARY_HOME/*" org.jlab.jnp.reader.EventWriter $*
