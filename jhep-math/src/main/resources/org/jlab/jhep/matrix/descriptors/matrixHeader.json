{
         "model": "VGD_012G",
     "reference": "A.B. et al, PRL",
    "web-source": "http://aaa.html",
       "formula": "$sf1=p0+p1*a/b*\cos(c)+p2*(a/b)^2*\cos(2c)+p3*a/b*\sin(c)$",
     "variables": ["SF1"],
          "axis": [
           	  { "name": "a", "bins": 25, "min": 0.0, "max": 1.5 },
           	  { "name": "b", "bins": 10, "min": 1.0, "max": 10.0 },
           	  { "name": "c", "bins": 120, "min": 0.0, "max": 3.1415926 }
       		  ],
   "parameters": [
            	 {"name":"p0", "value": 1.0},
            	 {"name":"p1", "value": 0.2},
            	 {"name":"p2", "value": 0.1},
            	 {"name":"p3", "value": 0.1}
       ]
}
